            "Mobileia\\Expressive\\Middleware\\": "src/Middleware/",
            "Mobileia\\Expressive\\StackDriver\\": "src/StackDriver/",
            "Mobileia\\Expressive\\Helper\\": "src/Helper/",
            "Mobileia\\Expressive\\Google\\": "src/Google/",
            "Mobileia\\Expressive\\Factory\\": "src/Factory/",
            "Mobileia\\Expressive\\Handler\\": "src/Handler/",
            "Mobileia\\Expressive\\Config\\": "src/Config/"
